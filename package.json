{
  "name": "generator-seedexpress",
  "version": "1.0.0",
  "description": "Generator ExpressJS + Swagger",
  "homepage": "https://github.com/leduong/generator-seedexpress",
  "author": {
    "name": "Le Duong",
    "email": "me@leduong.com",
    "url": "https://github.com/leduong"
  },
  "files": ["generators"],
  "main": "generators/index.js",
  "keywords": [
    "generator swagger", "generator expressjs", "swagger express", "sql", "yeoman-generator"
  ],
  "devDependencies": {
    "yeoman-test": "^1.7.0",
    "yeoman-assert": "^3.1.0",
    "coveralls": "^3.0.0",
    "eslint": "^5.4.0",
    "prettier": "^1.11.1",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.2",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-config-xo": "^0.24.2",
    "jest": "^23.5.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "asciify": "^1.3.5",
    "chalk": "^2.1.0",
    "lodash": "^4.17.11",
    "pluralize": "^7.0.0",
    "underscore.string": "^3.3.5",
    "yeoman-generator": "^3.1.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.jsx?$",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix", "git add"
    ],
    "*.json": ["git add", "prettier --write"]
  },
  "eslintConfig": {
    "env": {
      "jest": true,
      "node": true
    },
    "extends": [
      "prettier", "xo"
    ],
    "plugins": ["prettier"],
    "rules": {
      "indent": [
        "error", 2
      ],
      "semi": [
        "error", "always"
      ],
      "quotes": ["error", "single"]
    }
  },
  "scripts": {
    "pretest": "./node_modules/.bin/eslint --fix .",
    "precommit": "lint-staged",
    "test": "./node_modules/.bin/jest --coverage"
  },
  "repository": "leduong/generator-seedexpress",
  "license": "MIT"
}
